<p id="notice"><%= notice %></p>

<h1>Prds</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Targetreleaseversion</th>
      <th>Dateexpectedstartwork</th>
      <th>Documentstatus</th>
      <th>Documentowner</th>
      <th>Designerowner</th>
      <th>Developerowner</th>
      <th>Qaowner</th>
      <th>Goals</th>
      <th>Backgroundstrategicfit</th>
      <th>Assumptions</th>
      <th>Requirements</th>
      <th>Userinteractionnotes</th>
      <th>Questions</th>
      <th>Notdoing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prds.each do |prd| %>
      <tr>
        <td><%= prd.title %></td>
        <td><%= prd.targetReleaseVersion %></td>
        <td><%= prd.dateExpectedStartWork %></td>
        <td><%= prd.documentStatus %></td>
        <td><%= prd.documentOwner %></td>
        <td><%= prd.designerOwner %></td>
        <td><%= prd.developerOwner %></td>
        <td><%= prd.qaOwner %></td>
        <td><%= prd.goals %></td>
        <td><%= prd.backgroundStrategicFit %></td>
        <td><%= prd.assumptions %></td>
        <td><%= prd.requirements %></td>
        <td><%= prd.userInteractionNotes %></td>
        <td><%= prd.questions %></td>
        <td><%= prd.notDoing %></td>
        <td><%= link_to 'Show', prd %></td>
        <td><%= link_to 'Edit', edit_prd_path(prd) %></td>
        <td><%= link_to 'Destroy', prd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prd', new_prd_path %>
